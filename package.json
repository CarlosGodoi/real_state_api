{
  "name": "real_state_api",
  "version": "1.0.0",
  "description": "API para serviçoes administrativos de imobiliária",
  "engines": {
    "node": "18.x"
  },
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "start:prod": "node dist/src/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate": "prisma generate",
    "lint": "eslint --fix --ext .ts",
    "build": "tsup src --out-dir dist && cp -r src/{@types,docs,env,http,lib,repositories,use-cases,utils} dist/",
    "build_test": "tsc && tsc-alias && prisma migrate deploy",
    "vercel-build": "npx prisma migrate deploy"
  },
  "keywords": [],
  "author": "Eduardo",
  "license": "MIT",
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^11.2.0",
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/multer": "1.4.7",
    "@types/node": "20.3.0",
    "@types/uuid": "9.0.2",
    "eslint": "8.42.0",
    "prisma-erd-generator": "^1.2.5",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "^5.1.3",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "7.0.0",
    "@fastify/multipart": "7.7.0",
    "@fastify/static": "6.10.2",
    "@fastify/swagger": "8.5.1",
    "@fastify/swagger-ui": "1.9.0",
    "@prisma/client": "^4.16.2",
    "@prisma/engines": "^4.16.2",
    "bcryptjs": "2.4.3",
    "cloudinary": "^2.5.1",
    "dotenv": "16.1.4",
    "fastify": "^4.28.1",
    "fastify-multer": "2.0.3",
    "multer": "1.4.5-lts.1",
    "prisma": "^4.15.0",
    "swagger-themes": "1.2.28",
    "tsc-alias": "^1.8.8",
    "uuid": "9.0.0",
    "zod": "^3.21.4"
  }
}
